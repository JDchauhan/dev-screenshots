<% include partials/header.ejs %>
    <script src="/js/transaction.js"></script>
    </head>

    <body>
        <%- include('partials/navbar', {
            home: true,
            login: false,
            plan: true,
            account: true
        }); %>

            <div id="loader" class="container row">
                <img class="hexerve" src="/hexerve.png" />
            </div>

            <div class="body-container container" style="display:none">
                <div id="err-msg"></div>
                <div class="mb-5">

                    <div class="table-responsive table-dark mb-2">
                        <table class="table transactions">
                            <thead>
                                <tr>
                                    <th class="text-center text-muted text-uppercase font-italic" colspan="4">Transactions</th>
                                </tr>
                                <tr>
                                    <th>#</th>
                                    <th>ID</th>
                                    <th class="">Amount</th>
