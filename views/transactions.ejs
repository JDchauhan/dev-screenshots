                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                    </div>
                    <div class="table-responsive table-dark mb-5">
                        <table class="table subscriptions">
                            <thead>
                                <tr>
                                    <th class="text-center text-muted text-uppercase font-italic" colspan="5">subscriptions</th>
                                </tr>
                                <tr>
                                    <th>#</th>
                                    <th>ID</th>
                                    <th>Plan</th>
                                    <th class="">Start</th>
                                    <th>End</th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                    </div>
                </div>
                <br/>


                <footer class="footer fixed-bottom text-light">
                    <% include partials/footer.ejs %>
